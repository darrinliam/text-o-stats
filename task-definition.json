{
    "family": "run-task1-def-textostats",
    "executionRoleArn": "arn:aws:iam::007431153856:role/ecsTaskExecutionRole",
	"networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "textostats-app",
			"image": "007431153856.dkr.ecr.us-east-2.amazonaws.com/ecr-textostats:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
		}
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
